{
  "name": "vscode-maniascript",
  "displayName": "ManiaScript",
  "icon": "images/ms.png",
  "description": "ManiaScript Language Support for vs code",
  "version": "0.2.0",
  "publisher": "mmcfarland",
  "engines": {
    "vscode": "^1.10.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "scripts/build.js",
    "prebuild": "rimraf lib ; mkdirp lib/syntaxes"
  },
  "license": "MIT",
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "maniascript",
        "aliases": [
          "ManiaScript",
          "maniascript"
        ],
        "extensions": [
          ".Script.txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "maniascript",
        "scopeName": "source.ms",
        "path": "./lib/syntaxes/ManiaScript.tmLanguage"
      }
    ]
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "concat-stream": "^1.6.0",
    "converter": "0.0.5",
    "mkdirp": "^0.5.1",
    "node-yaml": "^3.0.3",
    "pad-stream": "^2.0.0",
    "rimraf": "^2.6.1",
    "split": "^1.0.0",
    "through": "^2.3.8"
  }
}
