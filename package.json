{
  "name": "vscode-maniascript",
  "displayName": "ManiaScript",
  "icon": "images/ms.png",
  "description": "ManiaScript Language Support for vs code",
  "version": "0.2.0",
  "publisher": "mmcfarland",
  "engines": {
    "vscode": "^1.10.0"
  },
  "scripts": {
    "vscode:prepublish": "scripts/build.js",
    "build": "scripts/build.js"
  },
  "license": "MIT",
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "maniascript",
        "aliases": [
          "ManiaScript",
          "maniascript"
        ],
        "extensions": [
          ".Script.txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "maniascript",
        "scopeName": "source.ms",
        "path": "./syntaxes/ManiaScript.tmLanguage"
      }
    ]
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "concat-stream": "^1.6.0",
    "converter": "0.0.5",
    "node-yaml": "^3.0.3",
    "pad-stream": "^2.0.0",
    "split": "^1.0.0",
    "through": "^2.3.8"
  }
}
