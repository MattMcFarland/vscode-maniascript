{
    "name": "vscode-maniascript",
    "displayName": "ManiaScript",
    "icon": "images/ms.png",
    "description": "ManiaScript Language Support for vs code",
    "version": "1.2.19",
    "publisher": "mmcfarland",
    "repository": {
        "type": "git",
        "url": "https://github.com/MattMcFarland/vscode-maniascript"
    },
    "engines": {
        "vscode": "^1.41.0"
    },
    "scripts": {
        "vscode:prepublish": "npm run build",
        "build": "scripts/build.js"
    },
    "license": "MIT",
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./lib/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "maniascript",
                "aliases": [
                    "ManiaScript",
                    "maniascript"
                ],
                "extensions": [
                    ".Script.txt",
                    ".script.txt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "snippets": [
            {
                "language": "maniascript",
                "path": "./snippets/maniascript.json"
            }
        ],
        "colors": [
            {
                "id": "maniascript.templatestring",
                "description": "Template string color",
                "defaults": {
                    "dark": "#020",
                    "light": "#ada",
                    "highContrast": "#000"
                }
            }
        ],
        "grammars": [
            {
                "language": "maniascript",
                "scopeName": "source.ms",
                "path": "./syntaxes/ManiaScript.json"
            },
            {
                "scopeName": "xml.maniascript",
                "path": "./syntaxes/xml-ManiaScript.json",
                "injectTo": [
                    "text.xml"
                ],
                "embeddedLanguages": {
                    "meta.embedded.script.block.ms": "maniascript"
                }
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^7.10.6",
        "maniascript-tmlanguage": "^2.0.1",
        "mkdirp": "^0.5.1",
        "rimraf": "^2.6.3",
        "vscode": "^1.1.34"
    },
    "dependencies": {
        "fuzzaldrin": "^2.1.0"
    }
}